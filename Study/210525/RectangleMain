package java0525sub;

public class RectangleMain {

	public static void main(String[] args) {

		
		Rectangle rc = new Rectangle("nemo", 20,15);
//		rc.getArea();
		Rectangle rc2 = new Rectangle("nemo2", 19,80);
//		rc2.getArea2(19,82);
		
		System.out.println(rc.name + ":" + rc.getArea());
		System.out.println(rc2.name + ":" + rc2.getArea());
	}

}
